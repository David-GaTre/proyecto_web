<!DOCTYPE html>
<html lang = "en" style = "height: 100%;">
    <head>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script src="https://unpkg.com/vue@3"></script>
        <link rel="stylesheet" href="../Style/color_palette.css">
        <link rel="stylesheet" href="../Style/ticketStyle.css">
        <link rel="stylesheet" href="../Style/sidebar.css">
        <meta charset = "UTF-8">
        <title>Perfil</title>
    </head>
    <body class = "h-100">
        <main class = "h-100">
            <div id = "app" class = "h-100">
            <headermenu></headermenu>

            <div class="h-100">
                <div class="row h-100">
                        <!-- Menu lateral -->
                        <sidebar></sidebar>
                    
                    <div class="col">
                        <div v-for = "ticketshistorial in grouped_tickets_history" class = "row">
                            <div v-for = "ticket in ticketshistorial" class = "col-sm-10 col-md-6 col-xl-4" style="display: flex; justify-content: center;">
                                <ticket :id = "ticket.id" :user_name="ticket.user_name" :titl = "ticket.title" :favt = "ticket.favour_type" :desc = "ticket.short_desc" :inst = "ticket.instructions" :pric = "ticket.price" :sty="ticket.sty" :but="ticket.but" @cancel-ticket="cancelTicket($event)" @complete-ticket="completeTicket($event)" :img="ticket.img"></ticket>
                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
        </main>
    </body>

    <script src="app.component.js"></script>
    <script src="headermenu.component.js"></script>
    <script src="sidebar.component.js"></script>
    <script src="ticket.component.js"></script>
    <script>
        const mountedApp = app.mount('#app');
    </script>
</html>